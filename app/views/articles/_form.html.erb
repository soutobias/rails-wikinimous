<%= simple_form_for(article) do |f| %>
  <div>
  <%= f.label :title %>
  </div>
  <div>
  <%= f.text_field :title %>
  </div>
  <div>
  <%= f.label :content %>
  </div>
  <div>
  <%= f.text_area :content, data: { editor: 'markdown' }, rows: 10 %>
  </div>
  <div>
  <%= f.submit %>
  </div>
<% end %>
